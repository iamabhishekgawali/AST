{
  "language": "Python (Flask)",
  "selectors": {
    "function": ["function_definition"],
    "class": ["class_definition"],
    "comment": ["comment"],
    "import": [
      {
        "type": "import_from_statement",
        "source": { "path": [{"type": "dotted_name"}] }
      },
      {
        "type": "import_statement",
        "source": { "path": [{"type": "dotted_name"}] }
      }
    ],
    "patterns": {
      "Endpoints Defined": {
        "type": "decorator",
        "textMatch": ".route",
        "value": {
          "path": [{"type": "argument_list"}, {"type": "string"}]
        }
      },
      "Database Queries": {
        "type": "call",
        "textMatch": ".query",
        "value": null
      },
      "Environment Variables Accessed": {
        "type": "call",
        "textMatch": "os.environ.get",
        "value": {
            "path": [{"type": "argument_list"}, {"type": "string"}]
        }
      }
    }
  },
  "dependencyMaps": {
    "Frameworks Detected": {
      "flask": "Flask",
      "django": "Django"
    },
    "Database Technologies Detected": {
      "pymysql": "MySQL",
      "psycopg2": "PostgreSQL",
      "sqlite3": "SQLite"
    },
    "Cloud SDKs Detected": {
      "boto3": "AWS"
    }
  }
}